{
  "name": "deepresearchagent-web-ui",
  "version": "1.0.0",
  "description": "DeepResearchAgent için modern web arayüzü",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-api": "python api.py",
    "start-streamlit": "streamlit run app.py --server.port 8501",
    "dev": "concurrently \"npm run start-api\" \"npm run start-streamlit\"",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "antd": "^5.12.0",
    "axios": "^1.6.0",
    "@ant-design/icons": "^5.2.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.8.0",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "concurrently": "^8.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000"
}
